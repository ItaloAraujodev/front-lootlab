{
  "extends": [
    "@rocketseat/eslint-config/next"
  ],
  "ignorePatterns": ["src/app/api/auth/**.ts"],
  "rules": {
    "camelcase": "off",
    "no-useless-constructor": "off",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/no-non-null-asserted-optional-chain": "off"
  }
}